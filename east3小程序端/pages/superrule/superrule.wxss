/* pages/superrule/superrule.wxss */
.container {
  background-color: #d3e5f6;
  padding: 30px;
  padding-bottom: 200px;
}

.header {
  text-align: center;
  font-size: 20px;
  font-weight: bold;
  margin-bottom: 20px;
}

.content {
  background-color: #fff;
  padding: 20px;
  border-radius: 20px;
}

.section {
  margin-bottom: 20px;
}

.title {
  font-size: 16px;
  color: #000;
  background-color: #f7f2d2;
  padding: 5px;
  border-radius: 5px;
  margin-bottom: 10px;
}

.text {
  font-size: 14px;
  color: #000;
  margin-bottom: 10px;
}
