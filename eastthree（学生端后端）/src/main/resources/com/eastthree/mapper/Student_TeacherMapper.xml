<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.eastthree.mapper.Student_TeacherMapper" >
    <select id="selectTeacherByStudent" resultType="integer">
        select teacherId from student_teacher
            where studentId=#{studentId}
    </select>
</mapper>